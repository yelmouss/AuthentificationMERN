{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/images/(.*)",
      "headers": { "Cache-Control": "max-age=31536000" },
      "dest": "/images/$1"
    }
    
  ]

 
}